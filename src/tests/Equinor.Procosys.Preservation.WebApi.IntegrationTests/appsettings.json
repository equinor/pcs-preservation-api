{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    }
  },
  "Swagger": {
    "AuthorizationUrl": "https://login.microsoftonline.com",
    "ClientId": "00000000-0000-0000-0000-000000000000",
    "Scopes": {}
  },
  "MainApi": {
    "BaseAddress": "https://webapi.microsoftonline.com/api/",
    "ApiVersion": "1.0",
    "TagSearchPageSize": 1000
  },
    "AttachmentOptions": {
    "MaxSizeMb": 100,
    "BlobClockSkewMinutes": 3,
    "ValidFileSuffixes": [
      ".txt",
      ".jpg",
      ".jpeg",
      ".png",
      ".gif",
      ".pdf",
      ".doc",
      ".docx",
      ".ppt",
      ".pptx",
      ".xls",
      ".xlsx",
      ".msg"
    ],
    "BlobContainer": "preservation"
  },
  "CacheOptions": {
    "PermissionCacheMinutes": 20,
    "PlantCacheMinutes": 20
  },
  "Synchronization": {
    "UserOid": "00000000-0000-0000-0000-000000000000",
    "Interval": "24:0:0"
  }
}