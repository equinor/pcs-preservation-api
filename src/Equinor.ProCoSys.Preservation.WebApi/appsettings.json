{
  "AllowedHosts": "*",
  "API": {
    "Authority": "<to-be-set>",
    "Audience": "<to-be-set>"
  },
  "ApplicationInsights": {
    "InstrumentationKey": "<to-be-set>"
  },
  "Authenticator": {
    "Instance": "<to-be-set>",
    "MainApiScope": "<to-be-set>",
    "PreservationApiClientId": "<to-be-set>",
    "PreservationApiObjectId": "<to-be-set>",
    "PreservationApiSecret": "<to-be-set>"
  },
  "BlobStorage": {
    "BlobClockSkewMinutes": 3,
    "BlobContainer": "preservation",
    "BlockedFileSuffixes": [
      ".exe",
      ".msi",
      ".docm",
      ".dotm",
      ".xlsm",
      ".xltm"
    ],
    "ConnectionString": "<to-be-set>",
    "MaxSizeMb": 100
  },
  "CacheOptions": {
    "PermissionCacheMinutes": 20,
    "PlantCacheMinutes": 20
  },
  "ConnectionStrings": {
    "AppConfig": "<to-be-set>",
    "PreservationContext": "<to-be-set>",
    "ServiceBus": "<to-be-set>"
  },
  "ServiceBus": {
    "Enable": false,
    "LeaderElectorUrl": "",
    "LeaderElectorRenewLeaseInterval": "60000",
    "ServiceBus:EnableInDevelopment": false
  },
  "ServiceBus:LeaderElectorUrl": "<to-be-set>",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "MainApi": {
    "BaseAddress": "<to-be-set>",
    "ApiVersion": "<to-be-set>",
    "TagSearchPageSize": 10000
  },
  "MigrateDatabase": false,
  "SeedDummyData": false,
  "Swagger": {
    "AuthorizationUrl": "<to-be-set>",
    "ClientId": "<to-be-set>",
    "Scopes": {}
  },
  "Synchronization": {
    "Interval": "1:0:0"
  },
  "TagOptions": {
    "TagIsNewHours": 48
  }
}